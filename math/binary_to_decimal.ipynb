   "source": [
    "# Write a Python program to convert a binary number to decimal number.\n",
    "\n",
    "binary = list(input('Enter the binary number '))\n",
    "decimal = 0\n",
    "\n",
    "for i in range(len(binary)):\n",
    "    digit = binary.pop()\n",
    "    if digit == '1':\n",
    "        decimal = decimal + pow(2,i)\n",
    "\n",
    "print('The decimal number is',decimal)"
